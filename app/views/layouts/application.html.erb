<!DOCTYPE html>
<html>
  <head>
    <title>NewApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if logged_in? %>
      <%= link_to "Old page", "http://localhost:3001/users" %><br>
      <%= link_to "logout", logout_url, method: :delete %><br>

      <b>This is New Application</b>
    <% end %>
    <%= yield %>
  </body>
</html>
